# CompilerName=clang
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/maca64
matlabdir = /Applications/MATLAB_R2023b.app
startdir = /Users/busiu/Documents/MATLAB/Examples/R2023b/vision/MonocularVisualSimultaneousLocalizationAndMappingExample

# Toolchain information
cc = /usr/bin/xcrun -sdk macosx11.3 clang
cxx = /usr/bin/xcrun -sdk macosx11.3 clang++
linker = /usr/bin/xcrun -sdk macosx11.3 clang
linkerxx = /usr/bin/xcrun -sdk macosx11.3 clang++

cflags =  -fno-common -arch arm64 -mmacosx-version-min=11.0 -fexceptions -isysroot /Library/Developer/CommandLineTools/SDKs/MacOSX.sdk -fwrapv -DMATLAB_MEX_FILE -O2 -DNDEBUG -std=c99 -I "$root" -I "$startdir" -I "$matlabdir/extern/include" -I "$matlabdir/extern/include/vslamcore" -I "$root/interface" -I "." -DMODEL=helperMonoVisualSLAM_mex -c 

cxxflags =  -fno-common -arch arm64 -mmacosx-version-min=11.0 -fexceptions -isysroot /Library/Developer/CommandLineTools/SDKs/MacOSX.sdk -fwrapv -fobjc-arc -std=c++14 -stdlib=libc++ -DMATLAB_MEX_FILE -O2 -DNDEBUG -std=c++11 -I "$root" -I "$startdir" -I "$matlabdir/extern/include" -I "$matlabdir/extern/include/vslamcore" -I "$root/interface" -I "." -DMODEL=helperMonoVisualSLAM_mex -c 

ldflags =  -Wl,-twolevel_namespace -undefined error -arch arm64 -mmacosx-version-min=11.0 -Wl,-syslibroot,/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk -bundle   -L"$matlabdir/bin/maca64" -lmx -lmex -lmat -lc++  -Wl,-rpath,@loader_path -o helperMonoVisualSLAM_mex.mexmaca64 -L"$matlabdir/bin/maca64" -lmwrgb2gray_tbb -lmwblas -lemlrt -lcovrt -lut -lmwmathutil  -Wl,-exported_symbols_list,helperMonoVisualSLAM_mex.map

linkobjs =  "$matlabdir/bin/maca64/libopencv_calib3d.4.5.2.dylib" "$matlabdir/bin/maca64/libopencv_core.4.5.2.dylib" "$matlabdir/bin/maca64/libopencv_features2d.4.5.2.dylib" "$matlabdir/bin/maca64/libopencv_flann.4.5.2.dylib" "$matlabdir/bin/maca64/libopencv_imgproc.4.5.2.dylib" "$matlabdir/bin/maca64/libopencv_ml.4.5.2.dylib" "$matlabdir/bin/maca64/libopencv_objdetect.4.5.2.dylib" "$matlabdir/bin/maca64/libopencv_video.4.5.2.dylib" "$matlabdir/bin/maca64/libopencv_calib3d.4.5.dylib" "$matlabdir/bin/maca64/libopencv_core.4.5.dylib" "$matlabdir/bin/maca64/libopencv_features2d.4.5.dylib" "$matlabdir/bin/maca64/libopencv_flann.4.5.dylib" "$matlabdir/bin/maca64/libopencv_imgproc.4.5.dylib" "$matlabdir/bin/maca64/libopencv_ml.4.5.dylib" "$matlabdir/bin/maca64/libopencv_objdetect.4.5.dylib" "$matlabdir/bin/maca64/libopencv_video.4.5.dylib" "$matlabdir/bin/maca64/libopencv_calib3d.dylib" "$matlabdir/bin/maca64/libopencv_core.dylib" "$matlabdir/bin/maca64/libopencv_features2d.dylib" "$matlabdir/bin/maca64/libopencv_flann.dylib" "$matlabdir/bin/maca64/libopencv_imgproc.dylib" "$matlabdir/bin/maca64/libopencv_ml.dylib" "$matlabdir/bin/maca64/libopencv_objdetect.dylib" "$matlabdir/bin/maca64/libopencv_video.dylib" "$matlabdir/bin/maca64/libmwvslamcore.dylib"

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $linkobjs $ldflags

rule linkxx
    command = $linkerxx $in $linkobjs $ldflags

# Build
build $builddir/helperMonoVisualSLAM_data.o : cc $root/helperMonoVisualSLAM_data.c
build $builddir/rt_nonfinite.o : cc $root/rt_nonfinite.c
build $builddir/helperMonoVisualSLAM_initialize.o : cc $root/helperMonoVisualSLAM_initialize.c
build $builddir/helperMonoVisualSLAM_terminate.o : cc $root/helperMonoVisualSLAM_terminate.c
build $builddir/helperMonoVisualSLAM.o : cc $root/helperMonoVisualSLAM.c
build $builddir/cameraIntrinsics.o : cc $root/cameraIntrinsics.c
build $builddir/CameraParametersImpl.o : cc $root/CameraParametersImpl.c
build $builddir/monovslam.o : cc $root/monovslam.c
build $builddir/xzsvdc.o : cc $root/xzsvdc.c
build $builddir/xnrm2.o : cc $root/xnrm2.c
build $builddir/xdotc.o : cc $root/xdotc.c
build $builddir/constrainToRotationMatrix3D.o : cc $root/constrainToRotationMatrix3D.c
build $builddir/norm.o : cc $root/norm.c
build $builddir/OneDimArrayBehaviorTransform.o : cc $root/OneDimArrayBehaviorTransform.c
build $builddir/checkTransformationMatrixCondition.o : cc $root/checkTransformationMatrixCondition.c
build $builddir/warning.o : cc $root/warning.c
build $builddir/_coder_helperMonoVisualSLAM_api.o : cc $root/interface/_coder_helperMonoVisualSLAM_api.c
build $builddir/_coder_helperMonoVisualSLAM_mex.o : cc $root/interface/_coder_helperMonoVisualSLAM_mex.c
build $builddir/eml_int_forloop_overflow_check.o : cc $root/eml_int_forloop_overflow_check.c
build $builddir/xscal.o : cc $root/xscal.c
build $builddir/xaxpy.o : cc $root/xaxpy.c
build $builddir/xrot.o : cc $root/xrot.c
build $builddir/xswap.o : cc $root/xswap.c
build $builddir/xzgetrf.o : cc $root/xzgetrf.c
build $builddir/helperMonoVisualSLAM_emxutil.o : cc $root/helperMonoVisualSLAM_emxutil.c
build $builddir/_coder_helperMonoVisualSLAM_info.o : cc $root/interface/_coder_helperMonoVisualSLAM_info.c
build $builddir/c_mexapi_version.o : cc $matlabdir/extern/version/c_mexapi_version.c

# Link
build $root/helperMonoVisualSLAM_mex.mexmaca64 : link $builddir/helperMonoVisualSLAM_data.o $builddir/rt_nonfinite.o $builddir/helperMonoVisualSLAM_initialize.o $builddir/helperMonoVisualSLAM_terminate.o $builddir/helperMonoVisualSLAM.o $builddir/cameraIntrinsics.o $builddir/CameraParametersImpl.o $builddir/monovslam.o $builddir/xzsvdc.o $builddir/xnrm2.o $builddir/xdotc.o $builddir/constrainToRotationMatrix3D.o $builddir/norm.o $builddir/OneDimArrayBehaviorTransform.o $builddir/checkTransformationMatrixCondition.o $builddir/warning.o $builddir/_coder_helperMonoVisualSLAM_api.o $builddir/_coder_helperMonoVisualSLAM_mex.o $builddir/eml_int_forloop_overflow_check.o $builddir/xscal.o $builddir/xaxpy.o $builddir/xrot.o $builddir/xswap.o $builddir/xzgetrf.o $builddir/helperMonoVisualSLAM_emxutil.o $builddir/_coder_helperMonoVisualSLAM_info.o $builddir/c_mexapi_version.o 
