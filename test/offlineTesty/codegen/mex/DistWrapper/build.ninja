# CompilerName=clang
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/maca64
matlabdir = /Applications/MATLAB_R2024a.app
startdir = /Users/busiu/Desktop/Uczelnia/Projekt przejsÃÅciowy/vSLAM/test/offlineTesty

# Toolchain information
cc = /usr/bin/xcrun -sdk macosx11.3 clang
cxx = /usr/bin/xcrun -sdk macosx11.3 clang++
linker = /usr/bin/xcrun -sdk macosx11.3 clang
linkerxx = /usr/bin/xcrun -sdk macosx11.3 clang++

cflags =  -fno-common -arch arm64 -mmacosx-version-min=11.0 -fexceptions -isysroot /Library/Developer/CommandLineTools/SDKs/MacOSX.sdk -fwrapv -ffp-contract=off  -DMATLAB_MEX_FILE  -O2 -DNDEBUG -fvisibility=hidden -std=c99 -I "$root" -I "$startdir" -I "$matlabdir/extern/include" -I "$root/interface" -I "." -DMODEL=DistWrapper_mex3 -DMW_NEEDS_VERSION_H -c -fPIC -Xpreprocessor -fopenmp -I "$matlabdir/toolbox/eml/externalDependency/omp/maca64/include" -DOpenMP_omp_LIBRARY="$matlabdir/sys/os/maca64/libiomp5.dylib" 

cxxflags =  -fno-common -arch arm64 -mmacosx-version-min=11.0 -fexceptions -isysroot /Library/Developer/CommandLineTools/SDKs/MacOSX.sdk -fwrapv -ffp-contract=off  -fobjc-arc -std=c++14 -stdlib=libc++ -DMATLAB_MEX_FILE  -O2 -DNDEBUG -fvisibility=hidden -std=c++11 -I "$root" -I "$startdir" -I "$matlabdir/extern/include" -I "$root/interface" -I "." -DMODEL=DistWrapper_mex3 -DMW_NEEDS_VERSION_H -c -fPIC -Xpreprocessor -fopenmp -I "$matlabdir/toolbox/eml/externalDependency/omp/maca64/include" -DOpenMP_omp_LIBRARY="$matlabdir/sys/os/maca64/libiomp5.dylib" 

ldflags =  -Wl,-twolevel_namespace  -arch arm64 -mmacosx-version-min=11.0 -Wl,-syslibroot,/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk -framework Cocoa -bundle  -stdlib=libc++ -L"$matlabdir/bin/maca64" -lmx -lmex -lmat -L"$matlabdir/extern/bin/maca64" -lMatlabDataArray  -Wl,-rpath,@loader_path -o DistWrapper_mex3.mexmaca64 -L"$matlabdir/bin/maca64" -lmwimterp2d -lemlrt -lcovrt -lut -lmwmathutil -fPIC -L"$matlabdir/sys/os/maca64" -lomp

linkobjs = 

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $linkobjs $ldflags

rule linkxx
    command = $linkerxx $in $linkobjs $ldflags

# Build
build $builddir/DistWrapper_data.o : cxx $root/DistWrapper_data.cpp
build $builddir/rt_nonfinite.o : cxx $root/rt_nonfinite.cpp
build $builddir/DistWrapper_initialize.o : cxx $root/DistWrapper_initialize.cpp
build $builddir/DistWrapper_terminate.o : cxx $root/DistWrapper_terminate.cpp
build $builddir/DistWrapper.o : cxx $root/DistWrapper.cpp
build $builddir/validateattributes.o : cxx $root/validateattributes.cpp
build $builddir/undistortImage.o : cxx $root/undistortImage.cpp
build $builddir/colon.o : cxx $root/colon.cpp
build $builddir/distortPoints.o : cxx $root/distortPoints.cpp
build $builddir/assertValidSizeArg.o : cxx $root/assertValidSizeArg.cpp
build $builddir/_coder_DistWrapper_api.o : cxx $root/interface/_coder_DistWrapper_api.cpp
build $builddir/_coder_DistWrapper_mex.o : cxx $root/interface/_coder_DistWrapper_mex.cpp
build $builddir/eml_int_forloop_overflow_check.o : cxx $root/eml_int_forloop_overflow_check.cpp
build $builddir/eml_mtimes_helper.o : cxx $root/eml_mtimes_helper.cpp
build $builddir/ImageTransformer.o : cxx $root/ImageTransformer.cpp
build $builddir/cameraParameters.o : cxx $root/cameraParameters.cpp
build $builddir/CameraParametersImpl.o : cxx $root/CameraParametersImpl.cpp
build $builddir/_coder_DistWrapper_info.o : cxx $root/interface/_coder_DistWrapper_info.cpp
build $builddir/cpp_mexapi_version.o : cxx $matlabdir/extern/version/cpp_mexapi_version.cpp

# Link
build $root/DistWrapper_mex3.mexmaca64 : linkxx $builddir/DistWrapper_data.o $builddir/rt_nonfinite.o $builddir/DistWrapper_initialize.o $builddir/DistWrapper_terminate.o $builddir/DistWrapper.o $builddir/validateattributes.o $builddir/undistortImage.o $builddir/colon.o $builddir/distortPoints.o $builddir/assertValidSizeArg.o $builddir/_coder_DistWrapper_api.o $builddir/_coder_DistWrapper_mex.o $builddir/eml_int_forloop_overflow_check.o $builddir/eml_mtimes_helper.o $builddir/ImageTransformer.o $builddir/cameraParameters.o $builddir/CameraParametersImpl.o $builddir/_coder_DistWrapper_info.o $builddir/cpp_mexapi_version.o 
