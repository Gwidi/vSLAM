build $builddir/_coder_testyCodegena_mex.o : cxx $root/interface/_coder_testyCodegena_mex.cpp
build $builddir/eml_int_forloop_overflow_check.o : cxx $root/eml_int_forloop_overflow_check.cpp
build $builddir/adjustNumScaleLevels.o : cxx $root/adjustNumScaleLevels.cpp
build $builddir/xscal.o : cxx $root/xscal.cpp
build $builddir/xaxpy.o : cxx $root/xaxpy.cpp
build $builddir/xrot.o : cxx $root/xrot.cpp
build $builddir/xswap.o : cxx $root/xswap.cpp
build $builddir/xzlascl.o : cxx $root/xzlascl.cpp
build $builddir/xzgetrf.o : cxx $root/xzgetrf.cpp
build $builddir/xgetrf.o : cxx $root/xgetrf.cpp
build $builddir/cameraIntrinsics.o : cxx $root/cameraIntrinsics.cpp
build $builddir/monovslam.o : cxx $root/monovslam.cpp
build $builddir/rigidtform3d.o : cxx $root/rigidtform3d.cpp
build $builddir/string1.o : cxx $root/string1.cpp
build $builddir/CameraParametersImpl.o : cxx $root/CameraParametersImpl.cpp
build $builddir/testyCodegena_mexutil.o : cxx $root/testyCodegena_mexutil.cpp
build $builddir/_coder_testyCodegena_info.o : cxx $root/interface/_coder_testyCodegena_info.cpp
build $builddir/cpp_mexapi_version.o : cxx $matlabdir/extern/version/cpp_mexapi_version.cpp

# Link
build $root/testyCodegena_mex1x24.mexmaca64 : linkxx $builddir/testyCodegena_data.o $builddir/rt_nonfinite.o $builddir/testyCodegena_initialize.o $builddir/testyCodegena_terminate.o $builddir/testyCodegena.o $builddir/validateattributes.o $builddir/constrainToRotationMatrix3D.o $builddir/xzsvdc.o $builddir/xzlangeM.o $builddir/xnrm2.o $builddir/xdotc.o $builddir/norm.o $builddir/checkTransformationMatrixCondition.o $builddir/warning.o $builddir/_coder_testyCodegena_api.o $builddir/_coder_testyCodegena_mex.o $builddir/eml_int_forloop_overflow_check.o $builddir/adjustNumScaleLevels.o $builddir/xscal.o $builddir/xaxpy.o $builddir/xrot.o $builddir/xswap.o $builddir/xzlascl.o $builddir/xzgetrf.o $builddir/xgetrf.o $builddir/cameraIntrinsics.o $builddir/monovslam.o $builddir/rigidtform3d.o $builddir/string1.o $builddir/CameraParametersImpl.o $builddir/testyCodegena_mexutil.o $builddir/_coder_testyCodegena_info.o $builddir/cpp_mexapi_version.o 
