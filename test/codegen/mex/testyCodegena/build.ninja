# CompilerName=clang
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/maca64
matlabdir = /Applications/MATLAB_R2024a.app
startdir = /Users/busiu/Desktop/Uczelnia/Projekt przejsÃÅciowy/vSLAM/test

# Toolchain information
cc = /usr/bin/xcrun -sdk macosx11.3 clang
cxx = /usr/bin/xcrun -sdk macosx11.3 clang++
linker = /usr/bin/xcrun -sdk macosx11.3 clang
linkerxx = /usr/bin/xcrun -sdk macosx11.3 clang++

cflags =  -fno-common -arch arm64 -mmacosx-version-min=11.0 -fexceptions -isysroot /Library/Developer/CommandLineTools/SDKs/MacOSX.sdk -fwrapv -ffp-contract=off  -DMATLAB_MEX_FILE  -O2 -DNDEBUG -fvisibility=hidden -std=c99 -I "$root" -I "$startdir" -I "$matlabdir/extern/include" -I "$matlabdir/extern/include/vslamcore" -I "$root/interface" -I "." -DMODEL=testyCodegena_mex1x24 -DMW_NEEDS_VERSION_H -c -fPIC -Xpreprocessor -fopenmp -I "$matlabdir/toolbox/eml/externalDependency/omp/maca64/include" -DOpenMP_omp_LIBRARY="$matlabdir/sys/os/maca64/libiomp5.dylib" 

cxxflags =  -fno-common -arch arm64 -mmacosx-version-min=11.0 -fexceptions -isysroot /Library/Developer/CommandLineTools/SDKs/MacOSX.sdk -fwrapv -ffp-contract=off  -fobjc-arc -std=c++14 -stdlib=libc++ -DMATLAB_MEX_FILE  -O2 -DNDEBUG -fvisibility=hidden -std=c++11 -I "$root" -I "$startdir" -I "$matlabdir/extern/include" -I "$matlabdir/extern/include/vslamcore" -I "$root/interface" -I "." -DMODEL=testyCodegena_mex1x24 -DMW_NEEDS_VERSION_H -c -fPIC -Xpreprocessor -fopenmp -I "$matlabdir/toolbox/eml/externalDependency/omp/maca64/include" -DOpenMP_omp_LIBRARY="$matlabdir/sys/os/maca64/libiomp5.dylib" 

ldflags =  -Wl,-twolevel_namespace  -arch arm64 -mmacosx-version-min=11.0 -Wl,-syslibroot,/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk -framework Cocoa -bundle  -stdlib=libc++ -L"$matlabdir/bin/maca64" -lmx -lmex -lmat -L"$matlabdir/extern/bin/maca64" -lMatlabDataArray  -Wl,-rpath,@loader_path -o testyCodegena_mex1x24.mexmaca64 -L"$matlabdir/bin/maca64" -lmwblas -lemlrt -lcovrt -lut -lmwmathutil -fPIC -L"$matlabdir/sys/os/maca64" -lomp

linkobjs =  "$matlabdir/bin/maca64/libopencv_calib3d.4.7.0.dylib" "$matlabdir/bin/maca64/libopencv_core.4.7.0.dylib" "$matlabdir/bin/maca64/libopencv_features2d.4.7.0.dylib" "$matlabdir/bin/maca64/libopencv_flann.4.7.0.dylib" "$matlabdir/bin/maca64/libopencv_imgproc.4.7.0.dylib" "$matlabdir/bin/maca64/libopencv_ml.4.7.0.dylib" "$matlabdir/bin/maca64/libopencv_objdetect.4.7.0.dylib" "$matlabdir/bin/maca64/libopencv_video.4.7.0.dylib" "$matlabdir/bin/maca64/libopencv_calib3d.407.dylib" "$matlabdir/bin/maca64/libopencv_core.407.dylib" "$matlabdir/bin/maca64/libopencv_features2d.407.dylib" "$matlabdir/bin/maca64/libopencv_flann.407.dylib" "$matlabdir/bin/maca64/libopencv_imgproc.407.dylib" "$matlabdir/bin/maca64/libopencv_ml.407.dylib" "$matlabdir/bin/maca64/libopencv_objdetect.407.dylib" "$matlabdir/bin/maca64/libopencv_video.407.dylib" "$matlabdir/bin/maca64/libopencv_calib3d.dylib" "$matlabdir/bin/maca64/libopencv_core.dylib" "$matlabdir/bin/maca64/libopencv_features2d.dylib" "$matlabdir/bin/maca64/libopencv_flann.dylib" "$matlabdir/bin/maca64/libopencv_imgproc.dylib" "$matlabdir/bin/maca64/libopencv_ml.dylib" "$matlabdir/bin/maca64/libopencv_objdetect.dylib" "$matlabdir/bin/maca64/libopencv_video.dylib" "$matlabdir/bin/maca64/libmwvslamcore.dylib"

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $linkobjs $ldflags

rule linkxx
    command = $linkerxx $in $linkobjs $ldflags

# Build
build $builddir/testyCodegena_data.o : cxx $root/testyCodegena_data.cpp
build $builddir/rt_nonfinite.o : cxx $root/rt_nonfinite.cpp
build $builddir/testyCodegena_initialize.o : cxx $root/testyCodegena_initialize.cpp
